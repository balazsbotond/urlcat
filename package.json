{
  "name": "urlcat",
  "version": "2.0.4",
  "description": "A library for concatenating URL's.",
  "repository": {
    "type": "git",
    "url": "https://github.com/balazsbotond/urlcat.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm test && npm run build"
  },
  "keywords": [
    "url",
    "uri",
    "build",
    "builder",
    "concat",
    "concatenate"
  ],
  "author": "Botond Balazs",
  "license": "MIT",
  "dependencies": {
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/qs": "6.9.5",
    "coveralls": "3.1.0",
    "jest": "26.4.2",
    "shx": "0.3.2",
    "ts-jest": "26.4.1",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  }
}
